{
  "name": "J-Video",
  "version": "2.3",
  "manifest_version": 3,
  "description": "Aisla y descarga videos online",
  "permissions": ["activeTab", "downloads", "scripting", "webRequest"],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icons/ico16.png",
      "32": "icons/ico32.png",
      "48": "icons/ico48.png",
      "128": "icons/ico128.png"
    }
  },
  "icons": {
    "16": "icons/ico16.png",
    "32": "icons/ico32.png",
    "48": "icons/ico48.png",
    "128": "icons/ico128.png"
  }
}
